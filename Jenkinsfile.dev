pipeline {
    agent any
    triggers {
        pollSCM '* * * * *'
    }
    stages {
        stage('Process Resources') {
            steps {
                sh './gradlew makeFrontend --stacktrace'
            }
        }
        stage('Run') {
            steps {
                sh './gradlew bootRun --stacktrace'
            }
        }
    }
}
